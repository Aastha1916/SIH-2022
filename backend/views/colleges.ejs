<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="./styles.css">
    <!-- <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous"> -->
    <title>Wizard-at your service</title>
</head>

<body>
    <div id="container11">
        <!--This is for the big navigation bar-->
        <a href="./index.html">
            <img src="./images/wizardlogo.png" alt="logo1" id="logo1" width="175" height="185">
        </a>
        <!--it is for website logo png-->
        <div id="navigation1">
            <!--links to concerned pages-->
            <div id="navigation1-1">
                <a href="./fake.html">Fake Universities</a>
            </div>
            <div id="navigation1-2">
                <a href="./aboutus.html">About us</a>
                <a href="./contactus.html">Contact us</a>
            </div>

            <!--at the top for navigating among pages-->
            <input type="button" value="Log In" id="Login-button1"
                onclick="window.location.href = './official_login.html'" />
            <input type="button" value="Sign Up" id="Sign-up-button1"
                onclick="window.location.href='./official_signup.html'" />


        </div>
    </div>
    <form action="/accept" method="post">
        <div style="padding-top: 120px; text-align: left;" id="table-box">
            <% for(var i=0;i<details.length;i++){ %>
                <table
                    style="border-collapse: collapse;  border: 2px solid gray; width: calc(50%-10px); float: left; margin: 5px;"
                    class="table">
                    <thead>
                        <% if(details.length){%>

                            <tr>
                                <th scope="col">University Name:- </th>
                                <% if(details.length){ %>
                                    <td>
                                        <b><i>
                                                <%=details[i].universityname %>
                                            </i>
                                        </b>
                                    </td>
                                    <% } %>
                            </tr>
                            <tr>
                                <th scope="col">Address:- </th>
                                <% if(details.length){ %>
                                    <td>
                                        <em>
                                            <%=details[0].address %>
                                        </em>
                                    </td>
                                    <% } %>
                            </tr>
                            <tr>
                                <th scope="col">Pincode:- </th>
                                <% if(details.length){ %>
                                    <td>
                                        <em>
                                            <%=details[0].pincode %>
                                        </em>
                                    </td>
                                    <% } %>
                            </tr>
                            <tr>
                                <th scope="col">State:- </th>
                                <% if(details.length){ %>
                                    <td>
                                        <em>
                                            <%=details[0].state %>
                                        </em>
                                    </td>
                                    <% } %>
                            </tr>
                            <tr>
                                <th scope="col">Geolocation:- </th>
                                <% if(details.length){ %>
                                    <td>
                                        <em>
                                            <%=details[0].geolocation %>
                                        </em>
                                    </td>
                                    <% } %>
                            </tr>
                            <tr>
                                <th scope="col">Contact Us:- </th>
                                <% if(details.length){ %>
                                    <td>
                                        <em>
                                            <%=details[0].contact %>
                                        </em>
                                    </td>
                                    <% } %>
                            </tr>
                            <tr>
                                <th scope="col">Fax Number:- </th>
                                <% if(details.length){ %>
                                    <td>
                                        <em>
                                            <%=details[0].faxnumber %>
                                        </em>
                                    </td>
                                    <% } %>
                            </tr>
                            <tr>
                                <th scope="col">Email:- </th>
                                <% if(details.length){ %>
                                    <td>
                                        <em>
                                            <%=details[0].email %>
                                        </em>
                                    </td>
                                    <% } %>
                            </tr>
                            <tr>
                                <th scope="col">Website:- </th>
                                <% if(details.length){ %>
                                    <td>
                                        <em>
                                            <%=details[0].website %>
                                        </em>
                                    </td>
                                    <% } %>
                            </tr>
                            <tr>
                                <th scope="col">Programmes:- </th>
                                <% if(details.length){ %>
                                    <td>
                                        <em>
                                            <%=details[0].programmes %>
                                        </em>
                                    </td>
                                    <% } %>
                            </tr>
                            <tr>
                                <th scope="col">AICTE:- </th>
                                <% if(details.length){ %>
                                    <td>
                                        <em>
                                            <%=details[0].aicte %>
                                        </em>
                                    </td>
                                    <% } %>
                            </tr>
                            <tr>
                                <th scope="col">NBA:- </th>
                                <% if(details.length){ %>
                                    <td>
                                        <em>
                                            <%=details[0].nba %>
                                        </em>
                                    </td>
                                    <% } %>
                            </tr>
                            <tr>
                                <th scope="col">NAAC:- </th>
                                <% if(details.length){ %>
                                    <td>
                                        <em>
                                            <%=details[0].naac %>
                                        </em>
                                    </td>
                                    <% } %>
                            </tr>
                            <tr>
                                <td>
                                    <div>
                                        <button type="submit">Accepted</button>
                                        <button type="submit">Rejected</button>
                                    </div>
                                </td>
                            </tr>
                            <%} else{%>
                                <h1>Sorry!! Everything is fine for now </h1>
                                <%}%>

                    </thead>

                </table>
                <!-- <button type="submit">Accepted</button>
            <button type="submit">Rejected</button> -->
                <% } %>

        </div>
    </form>
</body>

</html>